@model SquadManager.Models.UserViewModel

@{
    Layout = null;
}
<link href="~/css/login.css" rel="stylesheet" />
<section>
    <h1>Autenticação</h1>

    <form id="form">
    <input type="text" name="email"id="email"  value="@Model.Email" placeholder="Username / Email"/>
    <input type="password" name="password" id="password" value="@Model.Password" placeholder="Senha"/>
    <a href="#">Esqueceu a senha?</a>

    <button type="submit" onclick="login(event);"> Login </button>

    </form>

    <label>Não tem conta?<a href="/login/signup">Crie Aqui</a></label>
</section>

<script src="~/js/login.js"></script>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
