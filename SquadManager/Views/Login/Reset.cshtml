@model SquadManager.Models.UserViewModel

@{
    Layout = null;
}
<link href="~/css/login.css" rel="stylesheet" />

<section>
    <h1>Redefinir a Senha</h1>

    <form id="form">
        <input type="password" name="password" id="password" placeholder="Senha" />
        <input type="password" name="confirmPasswor" id="confirmPassword" placeholder="Confirmar a Senha" />
        <div class="error">
            <span hidden>Senhas Diferentes!</span>
        </div>
        

        <button type="submit"> Enviar </button>

    </form>

    <label>Não tem conta?<a href="/login/signup">Crie Aqui</a></label>
</section>

<script src="~/js/reset.js"></script>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
